clean:
	@make -sC asm clean
	@make -sC corewar clean

tests_run:
	@make -sC asm tests_run
	@make -sC corewar tests_run
	@gcovr

fclean:	clean
	@make -sC asm fclean
	@make -sC corewar fclean

re:	fclean compile

.PHONY:	all tests_run clean fclean re
